  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "history": "^4.7.2",
    "input-moment": "^0.4.0",
    "moment": "^2.22.1",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-dropzone": "^4.2.9",
    "react-modal-dialog": "^4.0.7",
    "react-redux": "^5.0.7",
