{
  "name": "therapy-records-cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-js": "node scripts/start.js",
    "start": "node server.js",
    "build-js": "node scripts/build.js",
    "build": "npm-run-all build-css build-js",
    "lint": "./node_modules/.bin/eslint --ignore-pattern '/src/**/*.test.js' ./src/. ",
    "test": "node scripts/test.js --env=jsdom",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "precommit": "npm run lint && npm run test-coverage",
    "test-coverage": "npm run lint && npm test -- --coverage"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.19.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^14.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "input-moment": "^0.4.0",
    "jest": "^24.8.0",
    "moment": "^2.22.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.2",
    "object-assign": "4.1.1",
    "path": "^0.12.7",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "15.6.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "15.6.2",
    "react-dropzone": "^4.2.9",
    "react-modal": "^3.8.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-rte": "^0.16.1",
    "react-test-renderer": "15.6.1",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-observable": "^0.18.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "resolve": "1.6.0",
    "rxjs": "5.5.10",
    "style-loader": "0.19.0",
    "superagent": "^3.8.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "nock": "^9.2.5",
    "redux-mock-store": "^1.5.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90.85,
        "functions": 84,
        "lines": 89
      }
    },
    "coveragePathIgnorePatterns": [
      "src/index.js",
      "src/store.js",
      "src/components/ProtectedRoute"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
